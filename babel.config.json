{
  "plugins": [
    "@babel/plugin-syntax-top-level-await",
    "@babel/plugin-syntax-jsx",
    "@babel/plugin-transform-modules-commonjs"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "esmodules": true,
          "node": true
        }
      }
    ]
  ]
}
