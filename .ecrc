  "Verbose": false,
  "Debug": false,
  "IgnoreDefaults": true,
  "SpacesAftertabs": false,
  "NoColor": false,
  "Exclude": [
    "/vendor/**/*",
    "/lib/**/*",
    "doc/.jekyll-cache/**/*",
    "doc/_site/**/*"
