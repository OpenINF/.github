{
  "Version": "2.3.5",
  "Verbose": false,
  "Debug": false,
  "IgnoreDefaults": true,
  "SpacesAftertabs": false,
  "NoColor": false,
  "Exclude": [
    "/vendor/**/*",
    "/lib/**/*",
    "doc/.jekyll-cache/**/*",
    "doc/_site/**/*",
    ".pnpm-lock.yaml",
    "Gemfile.lock",
    ".markdownlint.jsonc",
    ".vscode/tasks.json"
  ],
  "AllowedContentTypes": [],
  "PassedFiles": [],
  "Disable": {
    "EndOfLine": false,
    "Indentation": false,
    "InsertFinalNewline": false,
    "TrimTrailingWhitespace": false,
    "IndentSize": false,
    "MaxLineLength": false
  }
}
